<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonView">
        <attr name="pressScale" format="float" />
        <attr name="pressAnimDuration" format="integer" />
        <attr name="pressPivot" format="enum">
            <enum name="center" value="0" />
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="left" value="3" />
            <enum name="right" value="4" />
            <enum name="top_left" value="5" />
            <enum name="top_right" value="6" />
            <enum name="bottom_left" value="7" />
            <enum name="bottom_right" value="8" />
        </attr>
    </declare-styleable>
</resources>
